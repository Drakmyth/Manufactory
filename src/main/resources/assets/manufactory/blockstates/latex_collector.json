{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "manufactory:block/latex_collector"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "manufactory:block/latex_collector",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "manufactory:block/latex_collector",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "manufactory:block/latex_collector",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "north",
        "fill_status": "full"
      },
      "apply": {
        "model": "manufactory:block/latex_collector_full"
      }
    },
    {
      "when": {
        "facing": "east",
        "fill_status": "full"
      },
      "apply": {
        "model": "manufactory:block/latex_collector_full",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "fill_status": "full"
      },
      "apply": {
        "model": "manufactory:block/latex_collector_full",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "fill_status": "full"
      },
      "apply": {
        "model": "manufactory:block/latex_collector_full",
        "y": 270
      }
    }
  ]
}