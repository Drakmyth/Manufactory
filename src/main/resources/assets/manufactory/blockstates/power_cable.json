{
  "multipart": [
    {
      "apply": {
        "model": "manufactory:block/power_cable"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "manufactory:block/power_cable_side"
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "manufactory:block/power_cable_up"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "manufactory:block/power_cable_down"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "manufactory:block/power_cable_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "manufactory:block/power_cable_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "manufactory:block/power_cable_side",
        "y": 270
      }
    }
  ]
}